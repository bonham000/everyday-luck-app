{
  "name": "learn-mandarin",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "stargazer": "yarn stargazer:setup && npx ttab 'yarn start' && yarn stargazer:server",
    "stargazer:setup": "./scripts/stargazer_init.sh",
    "stargazer:revert": "./scripts/stargazer_revert.sh",
    "stargazer:init": "stargazer:init",
    "stargazer:view": "stargazer:view",
    "stargazer:server": "stargazer:server",
    "start": "expo start",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "tslint:fix": "tslint --project tsconfig.json -c tslint.json --fix",
    "prettier:fix": "prettier --config .prettierrc --write './{src,test}/**/*.{ts,tsx,json}'",
    "prettier:check": "prettier --config .prettierrc --list-different './{src,test}/**/*.{ts,tsx}'",
    "format": "npm run prettier:fix && npm run tslint:fix",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:update-snapshots": "npm test -- -u",
    "test:format": "npm run prettier:check && npm run tslint && npm run tsc",
    "test": "yarn run prettier:check && yarn run tslint && yarn run tsc && yarn run jest",
    "test:clear": "jest --clearCache && rm -rf build",
    "publish": "expo publish --non-interactive",
    "build": "expo build:android",
    "parse-words": "tsc && npx babel-node --presets env build/scripts/parse_vocabulary_list.js",
    "translate": "export GOOGLE_APPLICATION_CREDENTIALS=google-translation-api-key.json tsc && npx babel-node --presets env build/scripts/translate.js",
    "test-translate-api": "tsc && npx babel-node --presets env build/scripts/test_translate.js",
    "scrap": "tsc && npx babel-node --presets env build/scripts/scrap_audio.js",
    "audio-status": "tsc && npx babel-node --presets env build/scripts/audio_status.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "expo": "^32.0.0",
    "glamorous-native": "^1.4.2",
    "react": "^16.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-deck-swiper": "^1.5.26",
    "react-native-dialog": "^5.5.0",
    "react-native-flip-card": "^3.5.2",
    "react-native-paper": "^2.5.0",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-navigation": "^3.0.9",
    "sentry-expo": "^1.11.2"
  },
  "devDependencies": {
    "@types/expo": "^31.0.8",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "16.4.7",
    "@types/react-native": "0.55.17",
    "@types/react-native-dialog": "^4.0.2",
    "@types/react-native-sortable-list": "^0.0.7",
    "@types/react-navigation": "3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-inline-dotenv": "^1.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-expo": "^5.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.15.3",
    "react-native-confetti": "^0.1.0",
    "react-native-stargazer": "^1.0.5",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-immutable": "^5.1.0",
    "typescript": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  }
}
